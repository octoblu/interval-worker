{
  "name": "interval-worker",
  "version": "2.9.4",
  "description": "Simple interval service for nanocytes",
  "main": "command.js",
  "scripts": {
    "test": "mocha",
    "start": "node command.js",
    "coverage": "nyc npm test",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "mocha:json": "env NPM_ENV=test mocha --reporter json > coverage/mocha.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/interval-worker.git"
  },
  "keywords": [
    "nanocyte",
    "octoblu"
  ],
  "author": "Octoblu <npm@octoblu.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/interval-worker/issues"
  },
  "homepage": "https://github.com/octoblu/interval-worker#readme",
  "dependencies": {
    "async": "^2.0.0-rc.6",
    "coffee-script": "^1.10.0",
    "cron-parser": "^2.1.0",
    "debug": "^2.2.0",
    "fast-stats": "0.0.3",
    "ioredis": "^2.2.0",
    "kue": "^0.11.1",
    "lodash": "^4.13.1",
    "meshblu-config": "^6.0.0",
    "meshblu-http": "^7.0.0",
    "redlock": "^2.0.0",
    "uuid": "^2.0.2"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "coffee-script": "^1.10.0",
    "fakeredis": "^1.0.3",
    "mocha": "^3.0.2",
    "mocha-multi": "^0.9.1",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^8.3.0",
    "sinon": "^1.16.1",
    "sinon-chai": "^2.8.0"
  },
  "optionalDependencies": {
    "hiredis": "^0.4.1"
  },
  "nyc": {
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "extension": [
      ".coffee"
    ]
  }
}
